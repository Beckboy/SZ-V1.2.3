<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/act_register_finish"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dimen_40px"
                tools:context="com.junhsue.ksee.RegisterFinishActivity">

    <com.junhsue.ksee.view.CircleImageView
        android:id="@+id/img_finish_person"
        android:layout_width="@dimen/dimen_140px"
        android:layout_height="@dimen/dimen_140px"
        android:layout_marginTop="@dimen/dimen_180px"
        app:civ_border_width="@dimen/dimen_6px"
        app:civ_border_color="#E2D7CC"
        android:src="@drawable/pic_default_avatar" />

    <TextView
        android:id="@+id/welcome_SZ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="欢迎注册钬花"
        android:textSize="@dimen/f_60"
        android:textColor="#8392A0"
        android:layout_below="@+id/img_finish_person"
        android:layout_marginTop="@dimen/dimen_70px"/>

    <TextView
        android:id="@+id/line_finish"
        android:layout_width="@dimen/dimen_160px"
        android:layout_height="@dimen/dimen_2px"
        android:background="#CACACC"
        android:layout_below="@+id/welcome_SZ"
        android:layout_marginTop="@dimen/dimen_50px"/>

    <TextView
        android:id="@+id/tv_finish_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/f_48"
        android:textColor="#8392A0"
        android:layout_below="@+id/line_finish"
        android:layout_marginTop="@dimen/dimen_20px"/>

    <TextView
        android:id="@+id/tv_finish_company"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/f_28"
        android:textColor="#9DA1A7"
        android:layout_below="@+id/tv_finish_name"
        android:layout_marginTop="@dimen/dimen_20px"/>

    <Button
        android:id="@+id/btn_register_finish"
        android:layout_width="@dimen/dimen_300px"
        android:layout_height="@dimen/dimen_80px"
        android:text="登 录"
        android:textSize="@dimen/f_36"
        android:textColor="#CDAC8D"
        android:drawableLeft="@drawable/selector_login_jumpin"
        android:background="@drawable/shape_bg_login_btn"
        android:paddingLeft="@dimen/dimen_34px"
        android:paddingRight="@dimen/dimen_2px"
        android:paddingTop="@dimen/dimen_18px"
        android:paddingBottom="@dimen/dimen_18px"
        android:layout_marginTop="@dimen/dimen_360px"
        android:layout_below="@+id/tv_finish_company"
        android:onClick="onclick"/>

</RelativeLayout>
