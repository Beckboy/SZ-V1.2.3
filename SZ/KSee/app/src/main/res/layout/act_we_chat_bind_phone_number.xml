<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/act_we_chat_bind_phone_number"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.junhsue.ksee.WeChatBindPhoneNumberActivity">

    <LinearLayout
        android:id="@+id/ll_register1_actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <include layout="@layout/component_register1_actionbar"/>
    </LinearLayout>

    <ImageView
        android:id="@+id/img_register1_logo"
        android:layout_width="@dimen/dimen_102px"
        android:layout_height="@dimen/dimen_220px"
        android:layout_marginTop="@dimen/dimen_196px"
        android:layout_marginLeft="@dimen/dimen_50px"
        android:src="@drawable/pic_logo"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/dimen_152px"
        android:layout_below="@+id/img_register1_logo"
        android:layout_marginLeft="@dimen/dimen_40px"
        android:layout_marginRight="@dimen/dimen_40px">
        <include layout="@layout/component_register1_input"/>
    </LinearLayout>


</RelativeLayout>
